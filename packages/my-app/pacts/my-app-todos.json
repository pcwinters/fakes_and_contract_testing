{
  "consumer": {
    "name": "my-app"
  },
  "provider": {
    "name": "todos"
  },
  "interactions": [
    {
      "description": "a request for todos",
      "request": {
        "method": "GET",
        "path": "/todos"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": [
          {
            "id": "ce118b6e-d8e1-11e7-9296-cec278b6b50a",
            "description": "my todo description"
          }
        ],
        "matchingRules": {
          "$.body": {
            "min": 1
          },
          "$.body[*].*": {
            "match": "type"
          },
          "$.body[*].id": {
            "match": "regex",
            "regex": "^[0-9a-f]{8}(-[0-9a-f]{4}){3}-[0-9a-f]{12}$"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}